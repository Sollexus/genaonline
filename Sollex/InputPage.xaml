<?xml version="1.0" encoding="utf-8"?>
<Page
	x:Class="Sollex.InputPage" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Input page"
	WindowTitle="Input page">

    <DockPanel LastChildFill="True">
        <Button DockPanel.Dock="Top,Left" Click="BtnNext_OnClick" >Next</Button>
        <DataGrid
			    x:Name="TblInput" IsReadOnly="True"
			    AutoGenerateColumns="False"
			    DockPanel.Dock="Bottom"
			    Margin="8,8,8,8">
        </DataGrid>
    </DockPanel>
</Page>