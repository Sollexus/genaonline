@model GenaOnline.Models.LearningParamsViewModel

@{
	ViewBag.Title = "Gena online";
}

<div class="row">
	<div class="col-md-12">
		<h2>Choose csv file to load</h2>

		@using (Html.BeginForm("LoadFile", "Home", FormMethod.Post, new { enctype = "multipart/form-data" })) {

			@Html.EditorForModel()
			
			@Html.LabelFor(_ => _.CsvFile)
			@Html.TextBoxFor(_ => _.CsvFile, new {type = "file"})
			
			<input type="submit" />
		}
	</div>
</div>